      "source": [
        "rfm[\"R_Score\"] = pd.qcut(rfm[\"Recency\"], 4, labels=[4,3,2,1])\n",
        "rfm[\"F_Score\"] = pd.qcut(rfm[\"Frequency\"], 4, duplicates='drop').cat.codes + 1\n",
        "rfm[\"M_Score\"] = pd.qcut(rfm[\"Monetary\"], 4, labels=[1,2,3,4])"
