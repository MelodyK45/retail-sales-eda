    {
      "cell_type": "code",
      "source": [
        "# 1. Check initial shape\n",
        "print(\"Initial shape:\", df.shape)\n",
        "\n",
        "# 2. Remove duplicate rows\n",
        "df  = df.drop_duplicates()\n",
        "\n",
        "# Handle missing values\n",
        "# CustomerID is essential for customer analysis\n",
        "# Description is essential for product analysis\n",
        "df = df.dropna(subset=[\"CustomerID\", \"Description\"])\n",
        "\n",
        "# 4. Convert InvoiceDate to datetime\n",
        "df[\"InvoiceDate\"] = pd.to_datetime(df[\"InvoiceDate\"], errors=\"coerce\")\n",
        "\n",
        "# 5. Remove cancelled transsactions\n",
        "# Cancelled invoices start with 'C'\n",
        "df = df[~df[\"InvoiceNo\"].astype(str).str.startswith(\"C\")]\n",
        "\n",
        "# 6. Remove invalid values\n",
        "# Quatity and UnitPrice must be positive\n",
        "df = df[(df[\"Quantity\"] > 0) & (df[\"UnitPrice\"] > 0)]\n",
        "\n",
        "# 7. Create TotalPrice column\n",
        "df[\"TotalPrice\"] = df[\"Quantity\"] * df[\"UnitPrice\"]\n",
        "\n",
        "# 8. Final check\n",
        "print(\"Cleaned shape:\", df.shape)\n",
        "df.head()"
      ],
